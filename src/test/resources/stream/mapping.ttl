@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix mail: <http://example.com/mail#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://example.com/> .
@prefix rmls: <http://semweb.mmlab.be/ns/rmls#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://example.com/base> .
@prefix foaf: <http://foaf.org/foffie/> .
 <#TripleMap>

    a rr:TriplesMap;
    rml:logicalSource [
        rml:source [
            rdf:type rmls:TCPSocketStream ;
            rmls:hostName "localhost" ;
            rmls:port "9999" ;
            rmls:type "PULL"
        ];
        rml:referenceFormulation ql:JSONPath;
    ];
  rr:subjectMap [
    rr:template "http://example.com/Student/{ID}/{FirstName}";
    rr:class foaf:Person, ex:Student
  ];

  rr:predicateObjectMap [
    rr:predicate foaf:name;
    rr:objectMap [ rml:reference "FirstName" ]
  ].

