@prefix locn:   <http://www.w3.org/ns/locn#>.



# LIJSTProducttype

 <#LIJSTProducttype>

    a rr:TriplesMap;
    rml:logicalSource [
        rml:source "file:///home/wmaroy/github/rml-framework/akka-pipeline/src/main/resources/io/rml/framework/data/orders.xml";
        rml:referenceFormulation ql:XPath;
        rml:iterator "item"
    ];

    rr:subjectMap [
        rml:reference "/item/id";
        rr:termType rr:IRI;
        rr:class skos:Concept
    ];

    rr:predicateObjectMap [
            rr:predicateMap [
                rr:constant dcterms:title;
                rr:termType rr:IRI
            ];
            rr:objectMap [
                rml:reference "/item/id";
                rr:termType rr:Literal
            ]
        ];

    rr:predicateObjectMap [
        rr:predicateMap [
            rr:constant dcterms:title;
            rr:termType rr:IRI
        ];
        rr:objectMap [
            rr:template "test/{/item/id}";
            rr:termType rr:IRI
        ]
    ].
